{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-cond-assign": 0,
    "camelcase": 0,
    "curly": 0,
    "no-debugger": 0,
    "eqeqeq": 2,
    "no-eval": 2,
    "guard-for-in": 0,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": ["error", "nofunc"],
    "comma-style": [
      2,
      "last"
    ],
    "semi": "off",
    "no-loop-func": 2,
    "linebreak-style": 0,
    "new-cap": 0,
    "no-caller": 2,
    "no-empty": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "dot-notation": 0,
    "strict": 0,
    "no-multi-str": 2,
    "no-unused-expressions": 2
  }
}
